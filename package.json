{
  "name": "couch-buddy",
  "version": "0.1.0",
  "description": "Media Center with cast support",
  "scripts": {
    "client:i": "cd client && npm install",
    "client:dev": "cd client && npm run serve",
    "client:build": "cd client && npm run build",
    "server:i": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "install" : "npm-run-all --parallel  server:i client:i",
    "dev": "npm-run-all --parallel server:dev client:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:lucafaggianelli/couch-buddy.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucafaggianelli/couch-buddy/issues"
  },
  "homepage": "https://github.com/lucafaggianelli/couch-buddy",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
